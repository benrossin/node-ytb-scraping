{
  "name": "ytb-video",
  "version": "1.0.0",
  "description": "",
  "main": "./src/server.ts",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "npm run build && node dist/server.js",
    "dev": "nodemon src/server.ts",
    "migrate": "knex migrate:latest --knexfile src/knexfile",
    "make": "knex migrate:make --knexfile src/knexfile",
    "db": "sqlite3 src/modules/database/main.db",
    "prepare": "husky install",
    "lint": "npx eslint ./src",
    "format": "prettier --ignore-path .gitignore --write ./src//**/*.{json,ts,js}"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "knex": "^2.5.1",
    "node-cron": "^3.0.2",
    "puppeteer": "^21.3.6",
    "sqlite3": "^5.1.6",
    "twilio": "^4.18.0",
    "yup": "^1.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.18",
    "@types/node": "^20.8.0",
    "@types/node-cron": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "husky": "^8.0.3",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  }
